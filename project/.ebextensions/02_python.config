option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "starchaser.settings"
    "PYTHONPATH": "/opt/python/current/app/starchaser:/opt/python/current/app:/opt/python/current/app/pymote_env:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath:    /opt/python/current/app/starchaser/starchaser/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"

